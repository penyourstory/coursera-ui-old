{
  "name": "coursera-ui",
  "version": "0.0.0-semantically-released",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean": "rimraf lib",
    "build-lib": "npm run clean && babel src/ -d lib/",
    "commit": "git-cz",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lintfix": "eslint src --fix",
    "postinstalltemp": "bash .scripts/post_install.sh",
    "storybook": "start-storybook -p 9000 -s public",
    "build-storybook": "build-storybook -s public",
    "publish-storybook": "bash .scripts/publish_storybook.sh",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "mocha": "mocha --require .scripts/mocha_runner --recursive tests",
    "test:w": "mocha --require .scripts/mocha_runner --recursive tests -w",
    "test-only": "npm run mocha --silent tests",
    "test": "npm run test-only && npm run lint",
    "test:cover": "istanbul cover  _mocha -- --require .scripts/mocha_runner -R spec tests",
    "check-coverage": "istanbul check-coverage --statements 20 --branches 20 --functions 20 --lines 20",
    "report-coverage": "codecov -t 1dcea176-5252-4911-8621-060ab84e9c1e"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-graphql": "^1.0.1",
    "@kadira/storybook-addon-knobs": "^1.3.0",
    "airbnb-js-shims": "^1.0.1",
    "babel-cli": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-airbnb-flow": "^1.0.2",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "git-url-parse": "^6.0.5",
    "istanbul": "^1.0.0-alpha",
    "jsdom": "^9.6.0",
    "mocha": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.2",
    "react-scripts": "0.6.1",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.6",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "aphrodite": "^0.5.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-intl": "^2.1.5",
    "underscore": "^1.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vidaaudrey/coursera-ui.git"
  },
  "bugs": {
    "url": "https://github.com/vidaaudrey/coursera-ui/issues"
  },
  "homepage": "https://github.com/vidaaudrey/coursera-ui#readme",
  "license": "MIT",
  "engines": {
    "npm": "^3.0.0"
  }
}
